{
	"name": "evaluation-juillet-2024",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"proto:admin": "protoc --ts_opt server_none --ts_out ./app/lib/protos/ --proto_path proto proto/admin.proto",
		"proto:proprietaire": "protoc --ts_opt server_none --ts_out ./app/lib/protos/ --proto_path proto proto/proprietaire.proto",
		"proto:client": "protoc --ts_opt server_none --ts_out ./app/lib/protos/ --proto_path proto proto/client.proto",
		"proto:all": "pnpm proto:admin && pnpm proto:proprietaire && pnpm proto:client",
		"back:admin": "cargo run --bin admin",
		"back:client": "cargo run --bin client",
		"back:proprio": "cargo run --bin proprio"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@protobuf-ts/plugin": "^2.9.4",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^5.2.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/typography": "^0.5.13",
		"@types/eslint": "^8.56.7",
		"autoprefixer": "^10.4.19",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"flowbite": "^2.4.1",
		"flowbite-svelte": "^0.46.11",
		"flowbite-svelte-icons": "^1.6.1",
		"globals": "^15.0.0",
		"postcss": "^8.4.38",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.6.4",
		"svelte": "^4.2.7",
		"svelte-add": "^2.3.1",
		"svelte-check": "^3.6.0",
		"svelte2tsx": "^0.7.13",
		"tailwindcss": "^3.4.4",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0-alpha.20",
		"vite": "^5.0.3",
		"vite-plugin-kit-routes": "^0.6.2",
		"vitest": "^1.2.0"
	},
	"type": "module",
	"dependencies": {
		"@fontsource-variable/inter": "^5.0.18",
		"@fontsource/fira-mono": "^5.0.13",
		"@fontsource/freckle-face": "^5.0.20",
		"@fontsource/poppins": "^5.0.14",
		"@protobuf-ts/grpcweb-transport": "^2.9.4",
		"@protobuf-ts/runtime": "^2.9.4",
		"@protobuf-ts/runtime-rpc": "^2.9.4"
	}
}
